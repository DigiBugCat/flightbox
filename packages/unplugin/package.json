{
  "name": "@flightbox/unplugin",
  "version": "0.0.8",
  "description": "Universal build plugin for Flightbox auto-instrumentation (Vite, webpack, esbuild, Rollup)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./src/index.ts"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "default": "./src/vite.ts"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.js",
      "default": "./src/esbuild.ts"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.js",
      "default": "./src/webpack.ts"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.js",
      "default": "./src/rollup.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc"
  },
  "dependencies": {
    "@flightbox/core": "workspace:*",
    "@flightbox/register": "workspace:*",
    "@duckdb/node-api": "1.4.4-r.1",
    "ws": "^8.18.0",
    "unplugin": "^2.3.0"
  },
  "devDependencies": {
    "@types/ws": "^8.5.13",
    "vite": "^6.3.0"
  }
}
