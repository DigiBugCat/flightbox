{
  "name": "@flightbox/babel-plugin",
  "version": "0.0.14",
  "description": "Babel plugin for auto-instrumenting functions with Flightbox",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./src/index.ts"
