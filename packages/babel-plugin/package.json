  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc"
  },
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@babel/helper-plugin-utils": "^7.25.0",
    "@babel/types": "^7.26.0",
    "picomatch": "^4.0.0"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.0",
    "@types/picomatch": "^3.0.0"
  }
}
