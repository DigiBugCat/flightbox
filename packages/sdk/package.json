{
  "name": "@flightbox/sdk",
  "version": "0.0.9",
  "description": "Runtime SDK for Flightbox instrumentation",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./src/index.ts"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "import": "./dist/browser.js",
      "default": "./src/browser.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc"
  },
  "dependencies": {
    "@flightbox/core": "workspace:*",
    "@duckdb/node-api": "1.4.4-r.1"
  }
}
