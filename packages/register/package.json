{
  "name": "@flightbox/register",
  "version": "0.0.3",
  "description": "Node.js loader hook for Flightbox auto-instrumentation",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./src/index.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc"
  },
  "dependencies": {
    "@flightbox/sdk": "workspace:*",
    "acorn": "^8.14.0",
    "@sveltejs/acorn-typescript": "^1.0.5",
    "estree-walker": "^3.0.3",
    "magic-string": "^0.30.17",
    "picomatch": "^4.0.0",
    "esbuild": "^0.25.0"
  },
  "devDependencies": {
    "@types/picomatch": "^3.0.0",
    "@types/estree": "^1.0.0"
  }
}
